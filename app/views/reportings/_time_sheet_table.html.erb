<table class="table table-hover">
  <thead>
  <tr>
    <th>Project</th>
    <th>Description</th>
    <th>Logged Day</th>
    <th>Travel Duration</th>
    <th>Field Duration</th>
    <th>Office Duration</th>
    <th>Total Duration</th>
  </tr>
  </thead>
  <tbody>
  <% timings.each do |sheet|%>
    <tr>
      <td><%= sheet.project.project_number %>-<%= sheet.project.name %></td>
      <td><%= sheet.description %></td>
      <td><%= sheet.date.strftime("%A, %Y-%m-%e") %></td>
      <td><%= sheet.travel_duration %></td>
      <td><%= sheet.field_duration %></td>
      <td><%= sheet.office_duration %></td>
      <td><%= sheet.total_duration %></td>
    </tr>
  <% end %>
  </tbody>
</table>
<div class="float-right"> <%== pagy_bootstrap_nav(pagy) %></div>