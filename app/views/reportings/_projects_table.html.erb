<% if projects.present? %>
  <table class="table table-hover">
    <thead>
    <tr>
      <th>Project</th>
      <th>Total hours</th>
    </tr>
    </thead>
    <tbody>
    <% projects.each do |project|%>
      <tr>
        <td><%= project.project_number %>-<%= project.name %></td>
        <td><%= project.timings.pluck(:total_duration).sum %></td>
<!--        <td><%#= project.timings.includes(:surveyor).collect(&:surveyor).uniq.count %></td>-->
      </tr>
    <% end %>
    </tbody>
  </table>
<% else %>
  <div class="text-center pt-3 mt-3">
    <div class="alert alert-warning">No Records Found</div>
  </div>
<% end %>
<div class="float-right"> <%== pagy_bootstrap_nav(pagy) %></div>